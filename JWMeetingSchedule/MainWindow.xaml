<Window x:Class="JWMeetingSchedule.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JWMeetingSchedule"
        mc:Ignorable="d"
        Title="JW Meeting Schedule" Height="600" Width="800">
    <Grid Background="#121212">
        <StackPanel HorizontalAlignment="Left" Background="#2e2e2e" VerticalAlignment="Stretch" Width="50">
            <Button Height="50" Background="#2e2e2e" VerticalAlignment="Top" Width="50">
                <Image Source="Images/outline_assignment_white_36dp.png" Width="36"/>
            </Button>
            <Button Height="50" Background="#2e2e2e" VerticalAlignment="Top" Width="50">
                <Image Source="Images/outline_people_white_36dp.png" Width="36"/>
            </Button>
        </StackPanel>

    </Grid>
</Window>
